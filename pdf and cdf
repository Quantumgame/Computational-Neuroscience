import numpy as np
import matplotlib.pyplot as plt
from scipy.stats import norm
namuda=10
mean=namuda
sd=np.sqrt(namuda)
rv=norm(mean,sd)
plt.figure()
x=np.linspace(mean-3*sd,mean+3*sd)
plt.plot(x,rv.pdf(x))
plt.plot(x,rv.cdf(x))
#pdf is probability density function and cdf is cumulative density function
#with cdf we can indicate the possibility of some event

r1=1-rv.cdf(mean+sd)
r2=rv.cdf(2)
print r1,r2

#r1 is the possibility of value greater than mean+sd
#r2 is the possibilty of value less than 2 Hz
